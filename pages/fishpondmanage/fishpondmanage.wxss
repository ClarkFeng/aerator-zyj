/* pages/fishpondmanage/fishpondmanage.wxss */
.weui-grid {
  /* padding:14% 0 14% 0; */
  height: 78px;
}  
.mygrid::before{
      width: 1px;
      height: 20.5px;
}
.mygrid::after{
      width: 20.5px;
      height: 1px;
}

.mygrid::before,::after {
    content: " ";
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%); 
    background-color: #D9D9D9;
}

.weui-grids{
  background-color: rgb(130, 197, 109);
  margin: 5px;
  color: #ffffff;
  font-size: 19px;
}

.weui-grid {
position:relative;
float:left;
padding:10px 10px;
width:33.33333333%;
box-sizing:border-box;
border-right:1rpx solid #d9d9d9;
border-bottom:1rpx solid #d9d9d9;

}
