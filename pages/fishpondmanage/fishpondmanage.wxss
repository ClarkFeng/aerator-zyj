.weui-grid {
  /* padding:14% 0 14% 0; */
  height: 78px;
}  
.mygrid::before{
      width: 1px;
      height: 25.5px;
}
.mygrid::after{
      width: 25.5px;
      height: 1px;
}

.mygrid::before,::after {
    content: " ";
    position: absolute;
    top: 35%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%); 
    background-color: #ffffff;
}

